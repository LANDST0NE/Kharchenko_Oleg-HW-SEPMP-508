      "source": [
        "a = int(input())\n",
        "c = str()\n",
        "\n",
        "while a >= 27:\n",
        "  c += 'A'\n",
        "  a -= 26\n",
        "\n",
        "# сейчас a < 27 и из этого требуется вывести соответсвующую букву (B-Z)\n",
        "\n",
        "c += str(chr(64 + a))\n",
        "print(c)\n",
        "\n"
      ]
